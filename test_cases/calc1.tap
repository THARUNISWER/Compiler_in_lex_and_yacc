def fact (a) {
    see a==0 {
        return 1;
    }
    b = a*fact(a-1) ;
    return b;
}
def permutation ( n, r) {
    res = fact(n)/fact(n-r);
    return res;
}
def combination (n, r) {
    res = fact(n)/(fact(n-r)*fact(r));
    return res;   
}
